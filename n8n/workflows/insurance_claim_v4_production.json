      "main": [[{ "node": "통계 응답", "type": "main", "index": 0 }]]
    },
    "청구목록 Webhook": {
      "main": [[{ "node": "목록 API 호출", "type": "main", "index": 0 }]]
    },
    "목록 API 호출": {
      "main": [[{ "node": "목록 응답", "type": "main", "index": 0 }]]
    },
    "승인 Webhook": {
      "main": [[{ "node": "승인 API 호출", "type": "main", "index": 0 }]]
    },
    "승인 API 호출": {
      "main": [[{ "node": "승인 응답", "type": "main", "index": 0 }]]
    }
  },
  "active": false,
